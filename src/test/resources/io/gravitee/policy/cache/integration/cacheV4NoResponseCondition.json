{
    "id": "apiv4-cache-policy-no-response-condition",
    "name": "apiv4-cache-policy",
    "description": "apiv4-cache-policy",
    "definitionVersion": "4.0.0",
    "type": "proxy",
    "listeners": [
        {
            "type": "http",
            "paths": [
                {
                    "path": "/test-no-response-condition"
                }
            ],
            "entrypoints": [
                {
                    "type": "http-proxy"
                }
            ]
        }
    ],
    "endpointGroups": [
        {
            "name": "default",
            "type": "http-proxy",
            "endpoints": [
                {
                    "name": "default-endpoint",
                    "type": "http-proxy",
                    "configuration": {
                        "target": "http://localhost:8080/endpoint"
                    }
                }
            ]
        }
    ],
    "flows": [
        {
            "name": "cache-flow",
            "enabled": true,
            "selectors": [
                {
                    "type": "http",
                    "path": "/",
                    "pathOperator": "STARTS_WITH"
                }
            ],
            "request": [
                {
                    "name": "Cache",
                    "description": "test cache policy with V4 API",
                    "enabled": true,
                    "policy": "cache",
                    "configuration": {
                        "scope": "API",
                        "cacheName": "dummy-cache",
                        "key": "integration-test-cache",
                        "methods": ["GET"]
                    }
                }
            ],
            "response": [
                {
                    "name": "Transform headers",
                    "description": "",
                    "enabled": true,
                    "policy": "transform-headers",
                    "configuration": {}
                }
            ],
            "subscribe": [],
            "publish": []
        }
    ],
    "resources": [
        {
            "name": "dummy-cache",
            "enabled": true,
            "type": "dummy-cache",
            "configuration": {}
        }
    ]
}
